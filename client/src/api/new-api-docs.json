    "schemas": {
      "ProductItem": {
        "type": "object",
        "properties": {
          "_id": { "type": "string" },
          "__v": { "type": "number" },
          "name": { "type": "string" },
          "price": { "type": "number" },
          "description": { "type": "string" }
        },
        "required": ["_id", "name", "price"]
      },
      "GetProductVo": {
        "type": "object",
        "properties": {
          "product": { "$ref": "#/components/schemas/ProductItem" }
        }
      },
      "CreateProductDto": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "price": { "type": "number" },
          "description": { "type": "string" }
        },
        "required": ["name", "price"]
      },
      "GetAllProductsVo": {
        "type": "object",
        "properties": {
          "products": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/ProductItem" }
          }
        }
      },
      "UpdateProductDto": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
